{
	"compilerOptions": {
		"plugins": [{ "name": "typescript-plugin-css-modules" }],
		"strict": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"module": "es2022",
		"moduleResolution": "node",
		"target": "es2017",
		"allowJs": true,
		"jsx": "react",
		"paths": {
			"rtree-sql.js": ["./node_modules/@types/sql.js"]
		}
	},
	"include": ["./src/**/*"]
}
